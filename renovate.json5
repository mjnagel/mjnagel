{
  enabled: true,
  forkProcessing: 'enabled',
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    ':dependencyDashboard',
    'config:recommended',
    'helpers:pinGitHubActionDigests',
  ],
  ignorePresets: [
    ':ignoreModulesAndTests',
  ],
  branchConcurrentLimit: 0,
  prConcurrentLimit: 0,
  prHourlyLimit: 0,
  packageRules: [
    {
      matchFileNames: [
        '.github/**',
      ],
      groupName: 'actions',
      commitMessageTopic: 'actions',
    }
  ]
}